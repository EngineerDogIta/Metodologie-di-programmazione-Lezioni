public class HuskyDog {

    public String name;

    public HuskyDog () {
        this.name = "Lokino";
    }

    public HuskyDog (String name) {
        this.name = name;
    }

    public void  mangiaPollo () {
        System.out.println(name + ": pollo ti mangio!");
    }
    public void scappa () {
        System.out.println(name + ": corre come il vento!");
    }
}